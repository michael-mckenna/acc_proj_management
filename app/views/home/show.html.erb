<link href="https://fonts.googleapis.com/css?family=Lato:300,400,700" rel="stylesheet">
<div id="home">
	<div class="container">
		<img id="logo" src="<%= asset_path('logo.png') %>">

		<div id="temp">
			<% if current_user %>
	        Signed in as <strong><%= current_user.name %></strong>!
	        <%= link_to "Sign out", signout_path %>
	    	<% end %>
		</div>
		
	
		
		<div class="row">
			<div class="col-lg-12">
				<div id="content">
					<h1>Aggie Coding Club</h1>
					<h3>Project Mangement</h3>
					<%= link_to image_tag('google_signin.png', :class => "btn btn lg"), "/auth/google_oauth2"%>
					<p>Only Texas A&M University <br/> Personnel Allowed</p>
				</div>
			</div>
		</div>
	</div>
</div>

